<p id="notice"><%= notice %></p>

<h1>Listing Resumes</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>User</th>
      <th>Title</th>
      <th>Fullname</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Sex</th>
      <th>Birthday</th>
      <th>Height</th>
      <th>Marriage</th>
      <th>Tag</th>
      <th>Qq</th>
      <th>Intention jobs</th>
      <th>Wage</th>
      <th>District</th>
      <th>Photo img</th>
      <th>Photo audit</th>
      <th>Talent</th>
      <th>Complete percent</th>
      <th>Click</th>
      <th>Tpl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @resumes.each do |resume| %>
      <tr>
        <td><%= resume.status %></td>
        <td><%= resume.user_id %></td>
        <td><%= resume.title %></td>
        <td><%= resume.fullname %></td>
        <td><%= resume.email %></td>
        <td><%= resume.phone %></td>
        <td><%= resume.sex %></td>
        <td><%= resume.birthday %></td>
        <td><%= resume.height %></td>
        <td><%= resume.marriage %></td>
        <td><%= resume.tag %></td>
        <td><%= resume.qq %></td>
        <td><%= resume.intention_jobs %></td>
        <td><%= resume.wage %></td>
        <td><%= resume.district %></td>
        <td><%= resume.photo_img %></td>
        <td><%= resume.photo_audit %></td>
        <td><%= resume.talent %></td>
        <td><%= resume.complete_percent %></td>
        <td><%= resume.click %></td>
        <td><%= resume.tpl %></td>
        <td><%= link_to 'Show', resume %></td>
        <td><%= link_to 'Edit', edit_resume_path(resume) %></td>
        <td><%= link_to 'Destroy', resume, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Resume', new_resume_path %>
